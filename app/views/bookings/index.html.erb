<div class="container boxes">
  <% @bookings.each do |booking| %>
    <div class="row justify-content-between">
      <div class="col-7 mr-2 mb-3 card-deck teacher-card" >
        <div class="row justify-content-between">
          <div class="col-4">
            <strong><%= "#{booking.teacher.user.first_name} #{booking.teacher.user.last_name}"%></strong>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
